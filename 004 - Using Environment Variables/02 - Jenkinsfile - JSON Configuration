pipeline {
			image 'alpine:latest' 
		}
	}
	environment {
		json='''[
		{
		name: "java", 
		command: "echo 'java command'"
		},
		{
		name: "java2", 
		command: "echo 'java command2'"
		}
		]'''
			}
    stages {
        stage('build') {
            steps {
                script {
                	def buildConfigs = []
                	def buildConfigs = readJSON text: "$json"
                	buildConfigs.each{ buildConfig ->
                		sh "echo '$buildConfig.name' && $buildConfig.command"
					}
                }
            }
        }
    }
} 

